{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "name",
    "company"
  ],
  "properties": {
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "austin"
      ]
    },
    "company": {
      "type": "string",
      "default": "",
      "title": "The company Schema",
      "examples": [
        "visa"
      ]
    }
  },
  "examples": [{
    "name": "austin",
    "company": "visa"
  }]
}